{
    "nginx": {
        "image": {
            "$select$": [
                "nginx:latest",
                "nginx:alpine"
            ]
        },
        "ports": [
            "80:80",
            "443:443"
        ],
        "volumes": [
            "./html:/usr/share/nginx/html"
        ],
        "depends_on": [
            "php",
            "mysql"
        ]
    }
}