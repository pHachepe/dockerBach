{
    "apache": {
        "image": ["httpd:latest", "httpd:2.4", "httpd:2.2"],
        "working_dir": "/usr/local/apache2/htdocs",
        "volumes": [
            "./htdocs:/usr/local/apache2/htdocs",
            "./httpd.conf:/usr/local/apache2/conf/httpd.conf"
        ],
        "ports": ["8080:80"]
    }
}
