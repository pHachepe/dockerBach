{
    "elasticsearch": {
        "image": {
            "$select$": [
                "elasticsearch:latest",
                "elasticsearch:7",
                "elasticsearch:6"
            ]
        },
        "environment": {
            "ES_JAVA_OPTS": "-Xms512m -Xmx512m"
        },
        "ports": [
            "9200:9200",
            "9300:9300"
        ],
        "volumes": [
            "./data:/usr/share/elasticsearch/data"
        ]
    }
}