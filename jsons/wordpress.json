{
    "wordpress": {
        "image": ["wordpress:latest", "wordpress:5", "wordpress:4"],
        "depends_on": ["mysql"],
        "volumes": ["./target:/var/www/html"],
        "environment": {
            "WORDPRESS_DB_USER": "manager",
            "WORDPRESS_DB_PASSWORD": "secret",
            "WORDPRESS_DB_HOST": "db",
            "WORDPRESS_DB_NAME": "wordpress"
        },
        "ports": ["8080:80"]
    }
}
