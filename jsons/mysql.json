{
    "mysql": {
        "image": {
            "$select$": [
                "mysql:latest",
                "mysql:8.0",
                "mysql:5.7"
            ]
        },
        "environment": {
            "MYSQL_ROOT_PASSWORD": "rootpassword",
            "MYSQL_DATABASE": "dbname",
            "MYSQL_USER": "username",
            "MYSQL_PASSWORD": "password"
        },
        "ports": [
            "3306:3306"
        ],
        "volumes": [
            "./data:/var/lib/mysql"
        ]
    }
}